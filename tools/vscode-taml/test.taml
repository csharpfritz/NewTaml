# TAML Test File
# Test all syntax highlighting features

application			MyTestApp
version				1.0.0
enabled				true
disabled			false

# Numbers and booleans
numbers
	integer			42
	decimal			3.14
	negative		-100
	scientific		1.5e10
	
booleans
	yes_value		yes
	no_value		no
	on_value		on
	off_value		off
	true_value		true
	false_value		false

# Null values
null_values
	null_one		null
	null_two		nil
	null_three		~

# Environment variables
environment
	secret			${SECRET_KEY}
	api_key			${API_KEY}
	database_url	${DATABASE_URL}

# Parent keys (sections)
server
	host			localhost
	port			8080
	ssl
		enabled			true
		certificate		/path/to/cert.pem

# Nested structures
database
	primary
		host			db1.example.com
		port			5432
		username		admin
	replica
		host			db2.example.com
		port			5432
		read_only		true

# Lists
features
	authentication
	authorization
	logging
	monitoring
	caching

# Mixed content
user_settings
	theme			dark
	notifications	enabled
	languages
		english
		spanish
		french
	preferences
		auto_save		true
		font_size		14
		line_numbers	true

# Tab alignment examples (multiple tabs)
config
	short		value1
	medium_key		value2
	very_long_key_name		value3
